[calendars]

[[private]]
path = ~/ObsidianHome/Calendars/private
color = dark green
type = calendar

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = private
highlight_event_days = True
